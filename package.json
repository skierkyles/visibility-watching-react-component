{
  "name": "react-vizibility-watcher",
  "version": "0.0.5",
  "description": "A higher order component to watch a components visibility.",
  "repository": {
    "type": "git",
    "url": "https://github.com/skierkyles/visibility-watching-react-component"
  },
  "author": "Kyle Swanson",
  "license": "Unlicense",
  "homepage": "http://www.kyleswanson.org/",
  "keywords": [
    "react-component",
    "react"
  ],
  "scripts": {
    "webpacksample": "webpack sample/dist/index.js --output-file sample/dist/bundle.js",
    "babelsample": "babel --plugins 'transform-es2015-modules-umd' ./sample/src --out-dir ./sample/dist",
    "makesample": "npm run babelsample && npm run webpacksample",
