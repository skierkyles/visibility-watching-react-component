    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "peerDependencies": {
    "react": "~0.14.8 || ^15.0.0",
    "react-dom": "~0.14.8 || ^15.0.0",
    "get-visible-rect": "^0.1.2"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1"
